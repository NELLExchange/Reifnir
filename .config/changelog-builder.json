{
  "categories": [
    {
      "title": "## üöÄ Features",
      "labels": [
        "feature",
        "feat"
      ],
      "empty_content": "No new features this time."
    },
    {
      "title": "## üêõ Fixes",
      "labels": [
        "fix",
        "bug",
        "bugfix"
      ],
      "empty_content": "No fixes. Only new bugs."
    },
    {
      "title": "## ‚ùì Other changes",
      "labels": [],
      "mode": "PR"
    }
  ],
  "pr_template": "- #{{TITLE}} (##{{NUMBER}})",
  "commit_template": "- #{{TITLE}}",
  "ignore_labels": [
    "chore",
    "dependencies"
  ],
  "label_extractor": [
    {
      "pattern": "^(feat|feature|fix|bug|bugfix|build|ci|refactor|perf|test|style|docs|chore){1}(\\([\\w\\-\\.]+\\))?(!)?: ?([\\w ])+",
      "target": "$1",
      "flags": "gui",
      "on_property": "title"
    },
    {
      "pattern": "^(feat|feature|fix|bug|bugfix|build|ci|refactor|perf|test|style|docs|chore){1}\\/[\\w-]+[ :-]?([\\w ])+",
      "target": "$1",
      "flags": "gui",
      "on_property": "title"
    },
    {
      "pattern": "^(feat|feature|fix|bug|bugfix|build|ci|refactor|perf|test|style|docs|chore){1}\\/[\\w-]+",
      "target": "$1",
      "flags": "gui",
      "on_property": "branch"
    }
  ],
  "categorized_include_empty_content": true
}